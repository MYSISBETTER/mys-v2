local Toggle = SecTab:CreateToggle({
    Name = "Fly",
    CurrentValue = false,
    Flag = "Toggle1", -- Ensure unique flag for configuration saving
    Callback = function(Value)
        local player = game.Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
        local humanoid = character:WaitForChild("Humanoid")
        local flying = Value -- Toggle fly on/off based on the toggle state
        local speed = 50 -- Adjust the fly speed
        local bodyVelocity
        local bodyGyro
        local flyConnection

        -- Function to start flying
        local function startFlying()
            -- Create BodyVelocity to simulate flight
            bodyVelocity = Instance.new("BodyVelocity")
            bodyVelocity.MaxForce = Vector3.new(10000, 10000, 10000)  -- Adjust max force
            bodyVelocity.Velocity = Vector3.zero
            bodyVelocity.Parent = humanoidRootPart

            -- Create BodyGyro to stabilize the player
            bodyGyro = Instance.new("BodyGyro")
            bodyGyro.MaxTorque = Vector3.new(0, 0, 0)  -- Adjust max torque
            bodyGyro.CFrame = humanoidRootPart.CFrame
            bodyGyro.Parent = humanoidRootPart

            -- Update flight controls on every frame
            flyConnection = game:GetService("RunService").Heartbeat:Connect(function()
                local direction = Vector3.zero
                local camera = workspace.CurrentCamera

                -- WASD controls for flight direction
                if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.W) then
                    direction = direction + camera.CFrame.LookVector
                end
                if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.S) then
                    direction = direction - camera.CFrame.LookVector
                end
                if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.A) then
                    direction = direction - camera.CFrame.RightVector
                end
                if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.D) then
                    direction = direction + camera.CFrame.RightVector
                end

                -- Apply the velocity and update BodyGyro
                bodyVelocity.Velocity = direction * speed
                bodyGyro.CFrame = CFrame.new(humanoidRootPart.Position, humanoidRootPart.Position + direction)
            end)
        end
        
        -- Function to stop flying
        local function stopFlying()
            if flyConnection then
                flyConnection:Disconnect() -- Disconnect the Heartbeat to stop updating flight controls
                flyConnection = nil
            end

            -- Destroy BodyVelocity and BodyGyro when stopping the fly
            if bodyVelocity then
                bodyVelocity:Destroy()
                bodyVelocity = nil
            end
            if bodyGyro then
                bodyGyro:Destroy()
                bodyGyro = nil
            end
        end

        -- Toggle the fly state
        if flying then
            startFlying()  -- Start flying when toggle is on
        else
            stopFlying()   -- Stop flying when toggle is off
        end
    end
})




local Button = SecTab:CreateButton({
    Name = "Tp Tool",
    Callback = function()
        local player = game.Players.LocalPlayer
        local mouse = player:GetMouse()

        -- Create the teleport tool
        local tool = Instance.new("Tool")
        tool.Name = "Teleport Tool"
        tool.RequiresHandle = false
        tool.Parent = player.Backpack

        -- Function to teleport to mouse click
        local function teleportToClick()
            local character = player.Character or player.CharacterAdded:Wait()
            local rootPart = character:FindFirstChild("HumanoidRootPart")

            if rootPart and mouse.Target then
                local targetPosition = mouse.Hit.Position
                rootPart.CFrame = CFrame.new(targetPosition + Vector3.new(0, 3, 0)) -- Offset to avoid clipping
            end
        end

        -- Connect the tool's activation to the teleport function
        tool.Activated:Connect(teleportToClick)
    end
})



